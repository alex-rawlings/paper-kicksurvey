	issn = {0004-637X},
	journal = {\apj},
	keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics,black hole physics,galaxies: evolution,galaxies: interactions,galaxies: nuclei,gravitational waves},
	month = apr,
