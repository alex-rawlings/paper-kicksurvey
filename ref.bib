	journal = {\mnras},
	keywords = {Astrophysics - Astrophysics of Galaxies,Astrophysics - Instrumentation and Methods for Astrophysics,galaxies: star clusters: general,gravitation,methods: numerical,quasars: supermassive black holes},
	month = mar,
	pages = {4131--4148},
	title = {{{MSTAR}} - a Fast Parallelized Algorithmically Regularized Integrator with Minimum Spanning Tree Coordinates},
	url = {https://ui.adsabs.harvard.edu/abs/2020MNRAS.492.4131R},
	urldate = {2023-06-09},
	volume = {492},
	year = {2020},
	bdsk-url-1 = {https://ui.adsabs.harvard.edu/abs/2020MNRAS.492.4131R},
