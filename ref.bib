	issn = {0035-8711},
	journal = {\mnras},
	keywords = {Astrophysics - Astrophysics of Galaxies,black hole physics,galaxies: interactions,galaxies: kinematics and dynamics,galaxies: nuclei,gravitational waves,methods: numerical},
	month = apr,
	pages = {4753--4765},
	title = {Eccentricity Evolution of Massive Black Hole Binaries from Formation to Coalescence},
